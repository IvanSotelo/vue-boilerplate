{
  "name": "{{ name }}",
  "version": "0.0.0",
  "author": "{{ author }}",
  "description": "{{ description }}",
  "license": null,
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    {{#if e2e}}
    "e2e": "node test/e2e/runner.js",
    {{/if}}
    {{#if eslint}}
    "lint": "eslint --ext .js,.vue src {{#testing unit e2e}}test/unit test/e2e/specs{{/testing}}",
    {{/if}}
    {{#testing unit e2e}}
    "test": "{{#unit}}npm run unit{{/unit}}{{#unit}}{{#e2e}} && {{/e2e}}{{/unit}}{{#e2e}}npm run e2e{{/e2e}}",
    {{/testing}}
    {{#if unit}}
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    {{/if}}
    "build": "node build/build.js",
    "deploy": "npm run build; push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "epic-spinners": "^1.0.3",
    "gsap": "^1.20.4",
    "hamsterjs": "^1.1.3",
    "howler": "^2.0.9",
    "image-preloader": "^1.1.1",
    "isotope-layout": "^3.0.6",
    "masonry-layout": "^4.2.1",
    "smooth-scrolling": "^2.3.9",
    "sniffer": "github:watsondg/sniffer",
    "swiper": "^4.2.2",
    "push-dir": "^0.4.1",
    "vue": "^2.5.2"{{deps plugins}}
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.22.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "chromedriver": "^2.27.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "^0.28.0",
    {{#css}}
    {{#if_eq cssConfig 'scss'}}
    "sass-loader": "^7.0.1",
    "sass-resources-loader": "^1.3.3",
    "node-sass": "^4.8.3",
    {{/if_eq}}
    {{/css}}
    {{#eslint}}
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-vue": "^4.5.0",
    "eslint-plugin-promise": "^3.7.0",
    {{#if_eq eslintConfig 'standard'}}
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    {{/if_eq}}
    {{#if_eq eslintConfig 'airbnb'}}
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.7.0",
    {{/if_eq}}
    {{/eslint}}
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    {{#if unit}}
    "jest": "^22.0.4",
    "jest-serializer-vue": "^1.0.0",
    "vue-jest": "^2.6.0",
    {{/if}}
    {{#if e2e}}
    "selenium-server": "^3.0.1",
    "nightwatch": "^0.9.12",
    {{/if}}
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^2.1.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "pug": "^2.0.3",
    "rimraf": "^2.6.0",

    "multispinner": "^0.2.1",
    "semver": "^5.3.0",
    "shelljs": "^0.8.2",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "vue-loader": "^13.3.0",
    "url-loader": "^1.0.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
